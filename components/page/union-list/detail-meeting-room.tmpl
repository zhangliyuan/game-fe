<div class="team-table">
    <table class="table table-responsive">
        <thead>
        <tr>
            <td>频道名称</td>
            <td>在线人数</td>
            <td>状态</td>
            <td>操作</td>
        </tr>
        </thead>
        <tbody>
        {{#each list}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.online_count}}</td>
                <td>{{this.status}}</td>
                <td>{{this.operation}}</td>
            </tr>
        {{/each}}


        </tbody>
    </table>

</div>
<div class="register-table">
    <div class="col-lg-6 col-md-6">
        <div class="item">
            <span class="label">负责人姓名:</span>
            <span class="text" >{{name}}</span>
        </div>
        <div class="item">
            <span class="label">身份证号码:</span>
            <span class="text" >{{PID}}</span>
        </div>
        <div class="item">
            <span class="label">联系电话:</span>
            <span class="text" >{{phoneNo}}</span>
        </div>
        <div class="item">
            <span class="label">常用邮箱:</span>
            <span class="text" >{{email}}</span>
        </div>
        <div class="item">
            <span class="label">公会类型:</span>
            <span class="text" >{{type}}</span>
        </div>
        <div class="item">
            <span class="label">入住游戏:</span>
            <span class="text game-span" >{{games}}</span>
        </div>


    </div>

    <div class="col-lg-6 col-md-6">
        <div>
            <span>身份资料审核</span>
        </div>
        <div>
            <div class="register-check-info">
                <img src="{{img1}}" />
            </div>
            <div class="register-check-info">
                <img src="{{img2}}" />
            </div>
            <div class="register-check-info">
                <img src="{{img3}}" />
            </div>
            <div class="register-check-info">
                <img src="{{img4}}" />
            </div>

        </div>
    </div>

</div>
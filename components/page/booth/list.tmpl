<div class="booth-wrap">
    <div class="video-wrap">
        <table class="table table-striped table-bordered">
            <thead>
            <tr>
                <th>名称</th>
                <th>时长</th>
                <th>容量</th>
                <th>操作</th>
            </tr>
            </thead>
            <tbody>
            {{#each videoList }}
            <tr data-id="{{id}}" data-videosort="{{videoSort}}">
                <td>
                    {{videoName}}

                </td>
                <td>
                    {{videoTime}}
                </td>
                <td>
                   {{videoSize}}
                </td>
                <td>
                    <a  class="operation " data-oper="delete" > 删除</a>
                    <a data-src="{{videoUrl}}"  class="operation " data-oper="preview"> 预览</a>
                    <a> <i class="fa fa-arrow-up fa-lg"></i> </a>
                    <a> <i class="fa fa-arrow-down fa-lg"></i> </a>
                </td>
            </tr>
            {{/each}}



            </tbody>
        </table>
